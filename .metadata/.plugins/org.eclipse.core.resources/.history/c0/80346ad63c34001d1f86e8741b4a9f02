package colgantes1;

import java.io.File;
import java.util.Scanner;

public class ArchivoColgantes {

    private String path;
    
    public ArchivoColgantes(String path) {
	this.path = path;
    }
    
    public int[] cargarColgantes() {
	File fp = null;
	Scanner sc = null;
	
	try {
	    fp = new File(this.path);
	    sc = new Scanner(fp);
	    
	    int n = sc.nextInt();
	    for(int i=0; i<n; i++) {
		
	    }
	    
	} catch(Exception e) {
	    sc.close();
	} finally {
	    
	}
    }
    
}
